<h1>Create Super Hero:</h1>

<%= form_for @superhero do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :super_name %><br>
  <%= f.text_field :super_name %><br>
  <%= f.label :super_power %><br>
  <%= collection_select @superhero.name, :superpower_id, Superpower.all, :id, :name %><br>
  <%= f.submit %>
<% end %>
