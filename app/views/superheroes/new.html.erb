<h1>Create your Hero!!!</h1>
<%= form_for @superhero do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %>
  <br><br>
  Super Power
  <%= f.collection_select :superpower_id, Superpower.all, :id, :name %>

  <br><br>
  <%= f.button :submit%>
<% end %>
